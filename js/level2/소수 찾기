
var split_arr = null;

function solution(numbers) {
    var answer = 0;
    split_arr = numbers.split("");
    console.log(split_arr)
    
    let arr2 = [];
    for(let i=0; i<split_arr.length; i++){ // i는 자리수
        let temp = split_arr.map((e, index) => {
            console.log("e : "+e+" || index : "+index);
            //for(let j=0; j<)
            
            return e;
        });
        arr2.push(temp);
    }
    console.log(arr2)
    
    
    return answer;
}
